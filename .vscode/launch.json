{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "code",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}",
        "--enable-proposed-api",
        "ybor-studio.ede-vscode",
        "--disable-extension",
        "vscode.tunnel-forwarding",
        "--remote=ede+test"
      ],
      "preLaunchTask": "npm: compile"
    }
  ]
}