{
  "name": "ede-vscode",
  "displayName": "Ybor Studio EDE VSCode Extension",
  "description": "The Ybor Studio EDE VSCode Extension",
  "version": "0.0.9",
  "publisher": "ybor-studio",
  "repository": {
    "type": "git",
    "url": "https://github.com/ybor-studio/ede-vscode"
  },
  "license": "AGPL-3.0-only",
  "engines": {
    "vscode": "^1.102.0",
    "node": ">=20.0.0"
  },
  "categories": [
    "Other"
  ],
  "capabilities": {
    "virtualWorkspaces": true,
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "enabledApiProposals": [
    "resolvers",
    "portsAttributes",
    "tunnelFactory",
    "tunnels",
    "workspaceTrust",
    "contribViewsRemote",
    "extensionRuntime"
  ],
  "activationEvents": [
    "*"
  ],
  "extensionKind": [
    "workspace"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ede-vscode.tunnel.logs",
        "title": "Show Tunnel Logs",
        "category": "EDE",
        "enablement": "tunnelForwardingHasLog"
      },
      {
        "command": "ede-vscode.tunnel.restart",
        "title": "Restart Tunnels",
        "category": "EDE",
        "enablement": "tunnelForwardingIsRunning"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile:dts": "dts dev",
    "compile:tsc": "tsc -p ./",
    "compile": "npm run compile:dts && npm run compile:tsc",
    "watch": "tsc -watch -p ./",
    "package": "vsce package --allow-star-activation",
    "package:dev": "npm run package && code --install-extension ede-vscode-0.0.9.vsix --force"
  },
  "devDependencies": {
    "@types/node": "^20.x",
    "@types/vscode": "^1.102.0",
    "@vscode/dts": "^0.4.1",
    "@vscode/vsce": "^3.6.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "handlebars": "^4.7.8"
  }
}
