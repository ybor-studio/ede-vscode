{
  "name": "ede-vscode",
  "displayName": "Ybor Studio EDE VSCode Extension",
  "description": "The Ybor Studio EDE VSCode Extension",
  "version": "0.0.19",
  "publisher": "ybor-studio",
  "repository": {
    "type": "git",
    "url": "https://github.com/ybor-studio/ede-vscode"
  },
  "license": "AGPL-3.0-only",
  "engines": {
    "vscode": "^1.102.0",
    "node": ">=20.0.0"
  },
  "categories": [
    "Other"
  ],
  "capabilities": {
    "virtualWorkspaces": true,
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "enabledApiProposals": [
    "resolvers",
    "portsAttributes",
    "tunnelFactory",
    "tunnels",
    "workspaceTrust",
    "contribViewsRemote",
    "extensionRuntime"
  ],
  "activationEvents": [
    "*"
  ],
  "extensionKind": [
    "workspace"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ede-vscode.port-scan.start",
        "title": "Start Port Scan",
        "category": "EDE"
      },
      {
        "command": "ede-vscode.port-scan.stop",
        "title": "Stop Port Scan",
        "category": "EDE"
      },
      {
        "command": "ede-vscode.open.langflow",
        "title": "Open Langflow",
        "category": "EDE"
      }
    ],
    "remoteHelp": {
      "ede": [
        "EDE Remote Environment"
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile:dts": "dts dev",
    "compile:tsc": "tsup src/extension.ts --format cjs --out-dir out --external vscode --clean",
    "compile:langflow": "rm -fr ./out/langflow && cd node_modules/langflow && npm install && npm run build && cd - && mv -f node_modules/langflow/build/ ./out/langflow",
    "compile": "pnpm run compile:dts && pnpm run compile:tsc && pnpm run compile:langflow",
    "watch": "tsc -watch -p ./",
    "package": "vsce package --allow-star-activation --no-dependencies",
    "package:dev": "pnpm run package && code-server --extensions-dir /code-server/extensions --install-extension ede-vscode-0.0.17.vsix --force"
  },
  "devDependencies": {
    "@types/node": "^20.x",
    "@types/node-netstat": "^1.8.4",
    "@types/vscode": "^1.102.0",
    "@vscode/dts": "^0.4.1",
    "@vscode/vsce": "^3.6.0",
    "handlebars": "^4.7.8",
    "langflow": "git+https://github.com/langflow-ai/langflow.git#3c5645d6a320ff2ba7b347cd9ecd2f3cf214e2ed&path:/src/frontend",
    "node-netstat": "^1.9.0",
    "ps-list": "^9.0.0",
    "rxjs": "^7.8.2",
    "tailwindcss": "^4.1.17",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
