{
  "name": "ede-vscode",
  "displayName": "Ybor Studio EDE VSCode Extension",
  "description": "The Ybor Studio EDE VSCode Extension",
  "version": "0.0.6",
  "publisher": "ybor-studio",
  "repository": {
    "type": "git",
    "url": "https://github.com/ybor-studio/ede-vscode"
  },
  "license": "AGPL-3.0-only",
  "engines": {
    "vscode": "^1.102.0",
    "node": ">=20.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "extensionKind": [
    "workspace"
  ],
  "enabledApiProposals": [
    "resolvers",
    "portsAttributes",
    "tunnelFactory",
    "tunnels",
    "workspaceTrust"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ede-vscode.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile:dts": "dts dev",
    "compile:tsc": "tsc -p ./",
    "compile": "npm run compile:dts && npm run compile:tsc",
    "watch": "tsc -watch -p ./",
    "package": "vsce package --allow-star-activation"
  },
  "devDependencies": {
    "@types/node": "^20.x",
    "@types/vscode": "^1.102.0",
    "@vscode/dts": "^0.4.1",
    "@vscode/vsce": "^3.6.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "handlebars": "^4.7.8"
  }
}
